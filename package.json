{
  "name": "tesalate-api",
  "private": true,
  "description": "API for tesalate",
  "main": "./index.js",
  "repository": "https://github.com/tesalate/tesalate-processor.git",
  "author": "Devin Cunningham",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "commit": "cz",
    "release": "standard-version",
    "clean": "gts clean",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "yarn clean && tsc  -p tsconfig.build.json && yarn copy-files",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:coverage": "jest --silent --testLocationInResults --ci --all --coverage --json --outputFile=\"report.json\"",
    "test:watch": "jest -i --watchAll --verbose",
    "lint": "gts lint",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up api",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "compile": "tsc -p tsconfig.build.json",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "devDependencies": {
    "@types/ejson": "^2.2.0",
    "@types/ioredis-mock": "^5.6.0",
    "@types/jest": "^28.1.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "babel-eslint": "^10.1.0",
    "bson": "^4.6.4",
    "bullmq-utils": "^1.1.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "conventional-changelog-eslint": "^3.0.9",
    "cz-conventional-changelog": "3.3.0",
    "ejson": "^2.2.2",
    "eslint": "^8.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.0",
    "ioredis-mock": "^8.2.2",
    "jest": "^28.1.2",
    "jest-cli": "^28.1.2",
    "mongodb-memory-server": "^8.7.2",
    "redis-mock": "^0.56.3",
    "redis-server": "^1.2.2",
    "semantic-release": "^19.0.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bullmq": "^1.86.2",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.0.1",
    "geolib": "^3.3.3",
    "ioredis": "^5.0.6",
    "is-any-type": "^0.0.4",
    "joi": "^17.6.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.0",
    "mongoose-autopopulate": "^0.16.1",
    "pm2": "^5.2.0",
    "redis": "^4.1.0",
    "url-parse": "^1.5.10",
    "winston": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "3.3.1"
}
