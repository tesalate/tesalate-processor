{
  "name": "tesalate-api",
  "description": "API for tesalate",
  "main": "./index.js",
  "repository": "https://github.com/tesalate/tesalate-processor.git",
  "author": "Devin Cunningham",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "commit": "cz",
    "release": "standard-version",
    "clean": "gts clean",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "yarn clean && tsc && yarn copy-files",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:coverage": "jest --silent --testLocationInResults --ci --all --coverage --json --outputFile=\"report.json\"",
    "test:watch": "jest -i --watchAll --verbose",
    "lint": "gts lint",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up api",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "gts": "^3.1.0",
    "jest": "^28.1.1",
    "redis-mock": "^0.56.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bullmq": "^1.86.2",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.0.1",
    "ioredis": "^5.0.6",
    "is-any-type": "^0.0.4",
    "joi": "^17.6.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.4.0",
    "redis": "^4.1.0",
    "url-parse": "^1.5.10",
    "winston": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
